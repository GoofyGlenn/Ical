public class Calendar
	
	BufferedWriter bw;
	public Calendar (String filename) {
		try {
			FileWriter calwriter = new FileWriter(filename);
			bw = new BufferedWriter(calwriter);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
